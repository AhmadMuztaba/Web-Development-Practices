<%-include("header")-%>
<div class="box" id="heading">
<h1><%= newDay %></h1>
</div>
<div class="box">
   <% for(let i=0;i<items.length;i++){%>
    <div class="item">
    <input type="checkbox">
    <p><%=items[i] %></p>
    </div>
    <%}%>
    <form class="item" action="/" method="post">
        <input type="text" name="userItem" placeholder="next Item" autocomplete="off">
        <button type="submit" name="submit" value=<%=newDay%>>+</button>
    </form>
</div>
<%-include("footer")-%>